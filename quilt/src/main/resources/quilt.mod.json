{
  "schema_version": 1,
  "mixin": [
    "mcarf.mixins.json",
    "mcarf-common.mixins.json"
  ],
  "minecraft": {
    "environment": "*"
  },
  "quilt_loader": {
    "group": "${group}",
    "id": "mcarf",
    "version": "${version}",
    "metadata": {
      "name": "MCA: Reborn Fantasy Addon",
      "description": "This is an example description! Tell everyone what your mod is about!",
      "authors": ["'ArtofShannonLee', 'Xaidee' and Average Turtle."],
      "contact": {
        "sources": "https://github.com/architectury/architectury-templates"
      }

    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.mcarf.quilt.MCARFantasyQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.2"
      },
      {
        "id": "architectury",
        "version": ">=6.5.77"
      },
      {
        "id": "geckolib3",
        "version": ">=3.1.40"
      }
    ]
  }
}