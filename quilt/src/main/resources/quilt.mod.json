{
  "schema_version": 1,
  "mixin": [
    "mcarf.mixins.json",
    "mcarf-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "mcarf",
    "version": "${version}",
    "name": "MCA: Reborn Fantasy Addon",
    "description": "This is an example description! Tell everyone what your mod is about!",
    "authors": ["'ArtofShannonLee', 'Xaidee' and Average Turtle."],
    "contact": {
      "sources": "https://github.com/architectury/architectury-templates"
    },
    "license": "Insert License Here",
    "icon": "assets/mcarf/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "net.mcarf.quilt.MCARFantasy"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.2"
      },
      {
        "id": "architectury",
        "version": ">=6.5.77"
      }
    ]
  }
